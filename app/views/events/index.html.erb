<h1>Listing events</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Site</th>
    <th>Date</th>
    <th></th>
  </tr>

<% @events.each do |event| %>
  <tr>
    <td><%= event.name %></td>
    <td><%= event.site %></td>
    <td><%= event.date %></td>
    <td><%= link_to 'Show', event %></td>
  </tr>
<% end %>
</table>
